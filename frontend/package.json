{
	"name": "frontend",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"preview": "vite preview",
		"prepare": "cd .. && husky frontend/.husky",
		"check:all": "biome check . --diagnostic-level=error",
		"format:check": "biome format . --diagnostic-level=error",
		"format:fix": "biome format --write src/**/*.{css,tsx,ts,json}",
		"lint:check": "biome lint . --diagnostic-level=error",
		"lint:fix": "biome lint --write ./src/**/*.tsx",
		"typescript:check": "tsc --noEmit"
	},
	"dependencies": {
		"@ant-design/icons": "^5.6.1",
		"antd": "^5.23.4",
		"jwt-decode": "^4.0.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-router-dom": "^7.1.5"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "22.13.4",
		"@types/react": "^19.0.8",
		"@types/react-dom": "^19.0.3",
		"@vitejs/plugin-react": "^4.3.4",
		"globals": "^15.14.0",
		"husky": "^9.1.7",
		"typescript": "~5.7.2",
		"vite": "^6.1.0",
		"vite-plugin-checker": "0.8.0",
		"vite-plugin-svgr": "^4.3.0",
		"vite-tsconfig-paths": "5.1.4"
	},
	"engines": {
		"node": ">=22.13.0"
	}
}
